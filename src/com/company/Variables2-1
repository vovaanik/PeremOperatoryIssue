package com.company;

public class Main {

    public static void main(String[] args) {
        int Savings;
        Savings = 2_000_000_000;
        System.out.println(Savings);
        int Transfer;
        Transfer = 500_000_000;
        System.out.println(Transfer);
        int balance = Savings + Transfer;
        System.out.println(balance);
    }
}
